{
  "name": "passerine",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_expression"
      }
    },
    "_expression": {
      "type": "SYMBOL",
      "name": "assignment_expression"
    },
    "assignment_expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "left",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_assignment_op"
        },
        {
          "type": "FIELD",
          "name": "right",
          "content": {
            "type": "SYMBOL",
            "name": "_literal"
          }
        }
      ]
    },
    "_assignment_op": {
      "type": "STRING",
      "value": "="
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[_a-zA-Z][_0-9a-zA-Z]*"
    },
    "_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "number_literal"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        }
      ]
    },
    "number_literal": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "string_literal": {
      "type": "PATTERN",
      "value": "\"[^\\\"]*\""
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

